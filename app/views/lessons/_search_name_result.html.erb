<% search_name_results.each do |user| %>
  <tr class="odd gradeX" align="center">
  <td><%= link_to user.name, user_path(user.id) %></td>
  <td>
    <% if user.role == 1 %>
      <%= t ".teacher" %>
    <% else %>
      <%= t ".student" %>
    <% end %>
  </td>
  </tr>
<% end %>
<%= paginate search_name_results, theme: "twitter-bootstrap-3" %>
